{
    "name": "activitys_user_log2",
    "title": "参与活动者日志",
    "table": "activitys_user_log",
    "key": "log_id",
    "orderby_default": "`log_id` desc",
    "field_default": "`log_id`,`activitys_id`,`state`,`time_update`,`time_create`,`uin_user`,`uin_prize winners`,`prize_item_1`,`prize_item_2`,`prize_item_3`,`price_item_t`",
    "query": {
        "state_min": "`state` >= {0}",
        "state_max": "`state` <= {0}",
        "time_update_min": "`time_update` >= '{0}'",
        "time_update_max": "`time_update` <= '{0}'",
        "time_create_min": "`time_create` >= '{0}'",
        "time_create_max": "`time_create` <= '{0}'",
        "uin_user": "`uin_user` like '%{0}%'",
        "uin_prize winners": "`uin_prize winners` like '%{0}%'",
        "prize_item_1": "`prize_item_1` like '%{0}%'",
        "prize_item_2": "`prize_item_2` like '%{0}%'",
        "prize_item_3": "`prize_item_3` like '%{0}%'",
        "price_item_t": "`price_item_t` like '%{0}%'"
    },
    "update": {
        "state_add": "`state` = `state` + {0}"
    },
    "field_hide": []
}